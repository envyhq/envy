{
  "name": "nv",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_declaration"
      }
    },
    "module_keyword": {
      "type": "STRING",
      "value": "module"
    },
    "module_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "module_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "module_identifier"
        },
        {
          "type": "SYMBOL",
          "name": "module_block"
        }
      ]
    },
    "module_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "var_declaration"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "var_keyword": {
      "type": "STRING",
      "value": "var"
    },
    "var_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "var_keyword"
        },
        {
          "type": "SYMBOL",
          "name": "var_identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "type_identifier"
        }
      ]
    },
    "var_attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "attribute_identifier"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        }
      ]
    },
    "_declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "module_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "var_declaration"
        }
      ]
    },
    "_expression": {
      "type": "SYMBOL",
      "name": "_literal"
    },
    "var_identifier": {
      "type": "PATTERN",
      "value": "[a-z_]+"
    },
    "module_identifier": {
      "type": "PATTERN",
      "value": "[a-z_]+"
    },
    "attribute_identifier": {
      "type": "PATTERN",
      "value": "[a-z_]+"
    },
    "type_identifier": {
      "type": "PATTERN",
      "value": "(str|int|float|bool|url|nowt)"
    },
    "_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bool"
        },
        {
          "type": "SYMBOL",
          "name": "str"
        },
        {
          "type": "SYMBOL",
          "name": "float"
        },
        {
          "type": "SYMBOL",
          "name": "int"
        },
        {
          "type": "SYMBOL",
          "name": "nowt"
        }
      ]
    },
    "bool": {
      "type": "PATTERN",
      "value": "\\w(true|false)\\w"
    },
    "nowt": {
      "type": "PATTERN",
      "value": "\\w(nowt)\\w"
    },
    "str": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "float": {
      "type": "PATTERN",
      "value": "\\d+\\.\\d+"
    },
    "int": {
      "type": "PATTERN",
      "value": "\\d+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
